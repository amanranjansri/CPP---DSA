#include<iostream>
using namespace std;

bool search(int array[],int size)
{
    for(int i = 0; i<size; i++)
    {
        if(array[i]==5)
        return 1;
  
    }      return 0;
}

int main()
{
    int array[5] = {1, 2, 3, 4, 5};
    int ans = search(array, 5);
    cout << ans << endl;
    return 0;
}
