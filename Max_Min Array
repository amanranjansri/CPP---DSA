#include <iostream>
using namespace std;

int main()
{
    int array[5] = {20, 54, 2, 10, 8};
    int min = INT8_MAX;
    int max = INT8_MIN;

    for (int i = 0; i < 5; i++)
    {
        if (array[i] > max)
        {
            max = array[i];
        }
        if (array[i] < min)
        {
            min = array[i];
        }
    }
    cout << max << endl;
    cout << min << endl;

    return 0;
}
